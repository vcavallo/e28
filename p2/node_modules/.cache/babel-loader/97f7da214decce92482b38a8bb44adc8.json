 list.description }}</p>\n\n    <ul>\n      <div v-for=\"i in items\" :key=\"i.id\">\n        <div>\n          <ShoppingListItem :id=\"i.id\" />\n        </div>\n      </div>\n    </ul>\n\n    <p>\n      {{ list.instructions }}\n    </p>\n  </div>\n</template>\n\n<script>\nimport { axios } from \"@/api.js\";\nimport ShoppingListItem from '@/components/ShoppingListItem.vue';\n\nexport default {\n  props: [\"listID\"],\n  created() {\n    this.setUpList(this.listID);\n  },\n  data() {\n    return {\n      list: {},\n      items: [],\n    };\n  },\n  watch: {\n    listID(val) {\n      if (val) {\n        this.setUpList(val);\n      }\n    },\n  },\n  methods: {\n    setUpList(sID) {\n      this.getList(sID);\n      this.getItemsForList(sID);\n    },\n    getList(sID) {\n      axios.get(`shoppingList/${sID}`).then((res) => {\n        this.list = res.data.shoppingList;\n      });\n    },\n    getItemsForList(sID) {\n      axios.get(`shoppingListItem/query?shopping_list_id=${sID}`).then((res) => {\n        this.items = res.data.results;\n      });\n    },\n  },\n  components: {\n    ShoppingListItem\n  }\n};\n</script>\n"],"sourceRoot":"src/components"}]}